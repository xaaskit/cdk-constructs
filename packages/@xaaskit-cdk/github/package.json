{
  "name": "@xaaskit-cdk/github",
  "version": "0.0.0",
  "description": "CDK Construct for GitHub",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.xaaskit.cdk.github",
        "maven": {
          "groupId": "software.xaaskit.cdk",
          "artifactId": "xaaskit-cdk-github"
        }
      },
      "dotnet": {
        "namespace": "XaasKit.CDK.Constructs.GitHub",
        "packageId": "XaasKit.CDK.Constructs.GitHub",
        "signAssembly": false,
        "assemblyOriginatorKeyFile": "../../key.snk",
        "iconUrl": "https://raw.githubusercontent.com/aws/aws-cdk/master/logo/default-256-dark.png"
      },
      "python": {
        "distName": "xaaskit-cdk.github",
        "module": "xaaskit-cdk.github"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xaaskit/xaaskit-cdk.git",
    "directory": "packages/@xaaskit-cdk/github"
  },
  "homepage": "https://github.com/xaaskit/xaaskit-cdk",
  "scripts": {
    "clean": "rimraf coverage dist pack doc 'lib/**/*.js' 'lib/**/*.d.ts' 'tst/**/*.js' 'tst/**/*.d.ts'",
    "doc": "typedoc --excludeExternals --mode file --out doc lib/",
    "build": "jsii",
    "watch": "jsii -w",
    "build+test": "npm-run-all clean build test",
    "package": "npm-run-all clean build doc && jsii-pacmak",
    "cdk": "cdk",
    "upload": "npm publish pack/js/* && python3 -m twine upload pack/python/*"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "github"
  ],
  "author": "Vincent Lesierse",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.60.0",
    "@aws-cdk/aws-lambda": "^1.60.0",
    "@aws-cdk/core": "1.60.0",
    "constructs": "^3.0.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.61",
    "aws-cdk": "latest",
    "jsii": "^1.11.0",
    "jsii-pacmak": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "3.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.18.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.60.0",
    "@aws-cdk/aws-lambda": "^1.60.0",
    "@aws-cdk/core": "1.60.0",
    "constructs": "^3.0.4"
  }
}
